// syntax: https://jenkins.io/doc/book/pipeline/syntax/#agent
pipeline {
  agent {
    dockerfile {
      filename 'jobs/Test/Dockerfile'
      label 'dockerslave'
    }
  }
  stages {
    stage('Checkout'){
      steps {
        checkout scm
      }
    }
    stage('Build') {
      steps {
        sh 'ls /autotaglibro'
      }
    }
  }
}
